#!/bin/bash
if ! tmux has-session -t ğŸ¡; then
  tmux new -s ğŸ¡ -d

  tmux rename-window -t ğŸ¡:1 home
  tmux send-keys -t ğŸ¡:1 \
    cd Space ~ Enter
  
